                    if _G.Settings.Autofarmfollow then
                    pcall(function()
                        local level = game:GetService("Players").LocalPlayer.leaderstats.Level.Value

                        if level == 1 or level <= 15 then
                            Map = "Titan Dimension"
                        
                        elseif level == 16 or level <= 30 then
                            Map = "Demon Dimension"
                        
                        elseif level == 31 or level <= 45 then
                            Map = "Curse Dimension"
                        
                        elseif level == 46 or level <= 60 then
                            Map = "Villain Dimension"
                        
                        elseif level == 61 or level <= 75 then
                            Map = "Sword Dimension"
                        
                        elseif level == 76 or level <= 90 then
                            Map = "Ghoul Dimension"
                        
                        elseif level >= 91 then
                            Map = "Fate Dimension"
                        
                            end

                             wait(5)
     local args = {"CreateRoom",{["Difficulty"] = "Easy",["FriendsOnly"] = false,["MapName"] = Map,["Hardcore"] = false}}
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer(unpack(args))
            wait()
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("TeleportPlayers")
                            
                        end)

                end
            end
             end)
end
    })
    
    
    
