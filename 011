    local foldername = "prasert"
    local filename = tostring(game:GetService("Players").LocalPlayer.Name)
     
    function saveSettings()
        local HttpService,json = game:GetService("HttpService")
        pcall(function()
        if (writefile) then
            if isfolder(foldername) then
                json = HttpService:JSONEncode(_G.Settings)
                writefile(foldername.."\\"..filename..".lua", json)
            else
                makefolder(foldername)
                writefile(foldername.."\\"..filename..".lua", json)
            end
        end
        end)
    end
     
    function loadSettings()
        local HttpService = game:GetService("HttpService")
        pcall(function()
        if isfile(foldername.."\\"..filename..".lua") then
            _G.Settings = HttpService:JSONDecode(readfile(foldername.."\\"..filename..".lua"))
            end
         end)
        end
     
    loadSettings()

