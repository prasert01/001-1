                        elseif time == "18:00" or time == "18:01" or time == "18:02" or time == "18:03" or time == "18:04" or time == "18:05" or time == "18:06" or time == "18:07" or time == "18:08" or time == "18:09" or time == "18:10" then
                            if game.PlaceId == 6990131029 then
                                game:GetService("TeleportService"):Teleport(6938803436)
                        end
                            raidstart()
                            
                        elseif time == "19:00" or time == "19:01" or time == "19:02" or time == "19:03" or time == "19:04" or time == "19:05" or time == "19:06" or time == "19:07" or time == "19:08" or time == "19:09" or time == "19:10" then
                            if game.PlaceId == 6990131029 then
                                game:GetService("TeleportService"):Teleport(6938803436)
                        end
                            raidstart()
                            
                        elseif time == "20:00" or time == "20:01" or time == "20:02" or time == "20:03" or time == "20:04" or time == "20:05" or time == "20:06" or time == "20:07" or time == "20:08" or time == "20:09" or time == "20:10" then
                            if game.PlaceId == 6990131029 then
                                game:GetService("TeleportService"):Teleport(6938803436)
                        end
                            raidstart()
                            
                        elseif time == "21:00" or time == "21:01" or time == "21:02" or time == "21:03" or time == "21:04" or time == "21:05" or time == "21:06" or time == "21:07" or time == "21:08" or time == "21:09" or time == "21:10" then
                            if game.PlaceId == 6990131029 then
                                game:GetService("TeleportService"):Teleport(6938803436)
                        end
                            raidstart()
                            
                        elseif time == "22:00" or time == "22:01" or time == "22:02" or time == "22:03" or time == "22:04" or time == "22:05" or time == "22:06" or time == "22:07" or time == "22:08" or time == "22:09" or time == "22:10" then
