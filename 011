FarmingFolder:AddToggle({
    text = "เปิด auto ใส่ best card", 
    state = _G.Settings.Autobestcard,
    callback = function(a) 
        _G.Settings.Autobestcard = a
        saveSettings()


        spawn(function()
            while true do wait()
                if _G.Settings.Autobestcard then
                pcall(function()
                    local save = math.max(table.unpack(card))
                    for fh, v in pairs(game.Players.LocalPlayer.PlayerGui.MainGui.CenterUIFrame.Inventory.Frame.CardInventoryFrame.CardInventoryScrollingFrame:GetChildren()) do
                        if v.Name ~= "UIGridLayout" and "CardInventorySlot" then
                            
                            if tonumber(string.sub(v.LevelReq.text,5)) >= tonumber(save) then
                            for fo, fp in pairs(getconnections(v.CardClick.MouseButton1Down)) do
                          fp:Fire()
                            if game.Players.LocalPlayer.PlayerGui.MainGui.CenterUIFrame.Inventory.Frame.InvOverlayOuterFrame.CardInventoryOverlay.Rarity.Text == "Legendary" then
                    
                    game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)
                                end
                                end
                        end
                      end
                    end
                    
                    for fh, v in pairs(game.Players.LocalPlayer.PlayerGui.MainGui.CenterUIFrame.Inventory.Frame.CardInventoryFrame.CardInventoryScrollingFrame:GetChildren()) do
    if v.Name ~= "UIGridLayout" then
        if v.Name ~= "CardInventorySlot" then
        
        if tonumber(string.sub(v.LevelReq.Text,5)) < tonumber(save) then
            for fo, fp in pairs(getconnections(v.CardClick.MouseButton1Down)) do
                fp:Fire()
    if game:GetService("Players").LocalPlayer.PlayerGui.MainGui.CenterUIFrame.Inventory.Frame.InvOverlayOuterFrame.CardInventoryOverlay.OverlayUnequipButton.Label.Text == "UNEQUIP" then
game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("UnequipCard",v.Name)
            end
        end
    end
end
    end
    end
            end)
        end
            end
        end)
         
end
})


    FarmingFolder:AddToggle({
