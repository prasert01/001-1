hideff:AddToggle({
    text = "ลบชื่อ", 
    state = _G.Settings.Hidename,
    callback = function(a) 
        _G.Settings.Hidename = a
        saveSettings()


        spawn(function()
            while true do wait()
                if _G.Settings.Hidename then
                pcall(function()
                        game.Players.LocalPlayer.Character.Head.PlayerHealthBarGui:Destroy()
                                game:GetService("StarterGui"):SetCoreGuiEnabled(Enum.CoreGuiType.PlayerList, false)
                                game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function(a)
                                  va = a:WaitForChild("Humanoid")
                                  
                    end)
                                end)
                end
        break
        end
         end)
end
})

          
            
upcardff:AddToggle({
    text = "เปิด upgradecard", 
    state = _G.Settings.AutoUpgradecard,
    callback = function(a) 
        _G.Settings.AutoUpgradecard = a
        saveSettings()


        spawn(function()
            while true do wait()
                if _G.Settings.AutoUpgradecard then
                pcall(function()
                    local upgradelevelcard = string.sub(tostring(game:GetService("Players").LocalPlayer.PlayerGui.MainGui.CenterUIFrame.Inventory.Frame.InvOverlayOuterFrame.CardInventoryOverlay.UpgradeFrame.CardLevel.Text),-7,7)

        for fh, v in pairs(game.Players.LocalPlayer.PlayerGui.MainGui.CenterUIFrame.Inventory.Frame.CardInventoryFrame.CardInventoryScrollingFrame:GetChildren()) do
                if v.Name ~= "UIGridLayout" and "CardInventorySlot" then
                    for fo, fp in pairs(getconnections(v.CardClick.MouseButton1Down)) do
                    fp:Fire()
                        if game:GetService("Players").LocalPlayer.PlayerGui.MainGui.CenterUIFrame.Inventory.Frame.InvOverlayOuterFrame.CardInventoryOverlay.OverlayUnequipButton.Label.Text == "UNEQUIP" then
                            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("UpgradeCard",v.Name)
                        end
                    
                    end
                end

    end
        end)
            end
        end
         end)
end
})


